<div class="span 6 offset4 text-center">
	<h1>Add a Picture</h1>
	<div class="boxy text-center">
 
	 	<%= simple_form_for @pic do |f| %>
		 <%= f.input :hour %>
		 <%= f.input :image, :as => :file %>
		 <%= f.input :emotion %>
		 <%= f.input :learned %>

		 <%= submit_tag "Add Pic", :class => 'btn-primary btn large'%>
	</div>
 <% end %>
</div>