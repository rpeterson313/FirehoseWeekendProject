<div class>
	<h2 class "custom-pic-header text-center">Firehosed Photos</h2>

 
	<% @pics.each do |pic| %>
	<div class =span3>
		<div class ="boxy">
			<h2>Hour: <%= pic.hour %></h2>
			<p />

			<%= image_tag pic.image%>
		 
			<h4>Emotion:</h4> <p><%= pic.emotion %></p>
		 

			<h4>What I Learned:</h4>
			<p>
			 <%= pic.learned %></p>
			<br />
			<br />
		</div>
	</div>
	<% end %>
	
	<br />
</div>

